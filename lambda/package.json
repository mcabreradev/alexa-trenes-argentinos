{
  "name": "alexa-trenes-argentinos",
  "version": "1.0.0",
  "description": "Skill de Alexa para consultar horarios de trenes en Argentina",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "zip": "zip -r alexa-trenes-argentinos.zip index.js src/ node_modules/ -x \"*.git*\" \"*.DS_Store\"",
    "deploy": "pnpm prune --prod && pnpm run zip",
    "prune": "pnpm prune --prod"
  },
  "dependencies": {
    "ask-sdk-core": "^2.12.1",
    "ask-sdk-model": "^1.29.0",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "axios": "^1.6.8",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45"
  }
}
